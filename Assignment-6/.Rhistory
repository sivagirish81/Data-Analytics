str(BSensex)
Bst<-ts(BSensex$Close,frequency=365,start=c(2014,10,16),end = c(2019,10,15))
plot.ts(Bst)
#Naive Model
Naive<-naive(Bst)
plot.ts(Naive)
Bst<-ts(BSensex$Close,frequency=365,start=c(2014,10,16),end = c(2019,10,15))
plot.ts(Bst)
#Naive Model
Naive<-naive(Bst)
plot.ts(Naive)
mamod<-ma(Bst,order=365)
plot.ts(mamod)
mfore<-forecast(mamod)
accuracy(mfore)
armod<-ar(Bst)
plot.ts(armod)
frmod<-forecast(armod)
accuracy(frmod)
Bstdiff<-diff(Bst,differences = 1)
plot.ts(Bstdiff)
